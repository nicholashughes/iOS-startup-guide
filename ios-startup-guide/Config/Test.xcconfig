#include "Pods/Target Support Files/Pods-ios-startup-guide/Pods-ios-startup-guide.test.xcconfig"

// Architectures
ONLY_ACTIVE_ARCH = YES

// Build Options
DEBUG_INFORMATION_FORMAT = dwarf
ENABLE_TESTABILITY = YES

// Linking
LD_RUNPATH_SEARCH_PATHS = $(inherited) @executable_path/Frameworks

// Packaging
INFOPLIST_FILE = ios-startup-guide/Info.plist
PRODUCT_BUNDLE_IDENTIFIER = com.nicholashughes.ios-startup-guide.test
PRODUCT_NAME = $(TARGET_NAME)-Test

// Signing
CODE_SIGN_STYLE = Automatic

// Apple Clang - Code Generation
GCC_DYNAMIC_NO_PIC = NO
GCC_OPTIMIZATION_LEVEL = 0

// Apple Clang - Preprocessing
GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1 $(inherited)

// Asset Catalog Compiler - Options
ASSETCATALOG_COMPILER_APPICON_NAME = AppIconTest

// User-Defined
MTL_ENABLE_DEBUG_INFO = INCLUDE_SOURCE
ENVIRONMENT = test
